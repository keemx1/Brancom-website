{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/brancom main.html"
    },
    {
      "source": "/admin-login",
      "destination": "/admin-login.html"
    },
    {
      "source": "/client-login",
      "destination": "/client-login.html"
    },
    {
      "source": "/client-register",
      "destination": "/client-register.html"
    },
    {
      "source": "/client-dashboard",
      "destination": "/client-dashboard.html"
    },
    {
      "source": "/forgot-password",
      "destination": "/forgot-password.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
